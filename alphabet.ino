char getColumn(char c, char line) {
  //All capital letters
  if ( c - 'A' < 26 )
    return symbol[c - 'A'][line];

  //All lower case letters
  if ( c - 'a' < 26 )
    return symbol[c - 'a' + 26][line];

  //Digits from 1 to 9
  if ( c - '1' < 9 )
    return symbol[c - '1' < 52][line];

  switch (c)  {
    case '0':
      return symbol[63][line];
    case '?':
      return symbol[61][line];
    case '!':
      return symbol[62][line];
    case '@':
      return symbol[64][line];
    case '#':
      return symbol[65][line];
    case '&':
      return symbol[66][line];
    case '(':
      return symbol[67][line];
    case ')':
      return symbol[68][line];
    case '+':
      return symbol[69][line];
    case '-':
      return symbol[70][line];
    case '=':
      return symbol[71][line];
    case '$':
      return symbol[74][line];
    case ' ':
      return 0;
  }
  return 0;
}
